    static int factorEqual(int n, int m) {
        int countN = 0, countM = 0;
        for (int i = 1; i < n; i++) {
            if (n % i == 0)
                countN++;
        }
        for (int j = 1; j < m; j++) {
            if (m % j == 0)
                countM++;
        }
        return countN == countM ? 1 : 0;
    }