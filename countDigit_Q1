    static int noDigit(int n, int d) {
        int count = 0;
        while (n != 0) {
            if (n < 0) {
                n *= -1;
                if (n % 10 == d)
                    count += 1 * -1;
            } else if (d == n % 10)
                count++;
            n /= 10;
        }
        return count;
    }
